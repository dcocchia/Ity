{
  "name": "ity",
  "version": "0.2.0",
  "description": "A miniscule, depedency free JavaScript MVC",
  "main": "Ity.js",
  "scripts": {
    "test": "node build/mini-mocha.js build/test/*.js",
    "coverage": "rm -rf coverage && NODE_V8_COVERAGE=coverage node build/mini-mocha.js build/test/*.js && node reportCoverage.js",
    "pretest": "tsc && cp build/Ity.js Ity.js"
  },
  "devDependencies": {
    "mocha": "2.1.x",
    "gulp": "3.*",
    "gulp-uglify": "1.2.*",
    "gulp-rename": "1.2.*",
    "gulp-sourcemaps": "1.5.2",
    "vinyl-buffer": "1.0.0",
    "typescript": "^5.4.2",
    "ts-node": "^10.9.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dcocchia/Ity.git"
  },
  "keywords": [
    "ity",
    "mvc",
    "model",
    "view",
    "spa"
  ],
